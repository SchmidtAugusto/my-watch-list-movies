<div class='row mt-3 align-items-center'>
  <div class="col-9">
    <h1>My lists</h1>

  </div>
  <div class="col-3">
    <%= link_to "Create list", new_list_path, class: "btn btn-outline-secondary m-auto"%>
  </div>
  <div class='row p-3'>
    <% @lists.each do |list| %>
      <div class="col-6">
        <%= link_to list.name, list_path(list), class: "btn btn-outline-secondary w-100 mt-3"%>
      </div>
    <% end %>
</div>
